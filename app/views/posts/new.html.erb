<div id="page_wrapper">
  <h1> Make something </h1>

  <%= form_for :post, url: posts_path do |f| %>
    <p>
    <%= f.label :tittle %><br>
    <%= f.text_field :tittle %>
    </p>
    
    <p>
    <%= f.label :boby %><br>
    <%= f.text_area :body %>
    </p>

    <p>
      <%= f.submit %>
    </p>
  <% end %>
</div>
